@model List<Hospital>

<h1>Hospitales EF</h1>

<table class="table-bordered">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Direccion</th>
            <th>Telefono</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Hospital hosp in Model)
        {
        <tr>
            <td>@hosp.Nombre</td>
            <td>@hosp.Direccion</td>
            <td>@hosp.Telefono</td>
        </tr>
        }
    </tbody>
</table>